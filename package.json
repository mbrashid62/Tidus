{
  "name": "tidus",
  "version": "1.0.0",
  "description": "Spotify App built with react, redux, es6, and more",
  "scripts": {
    "start": "babel-node tools/distServer.js",
    "start-dev": "babel-node tools/srcServer.js",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js src/**/*.test.js src/components/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "test-lint-watch": "npm-run-all --parallel test:watch lint:watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "ship-it": "npm run prebuild && npm run build && git add . && git commit -m 'production build' && git push origin master && git push heroku master",
    "webpack": "webpack"
  },
  "author": "Bill Rashid",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap": "3.3.6",
    "classnames": "^2.2.5",
    "colors": "1.1.2",
    "compression": "^1.7.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "express": "^4.16.3",
    "google-oauth2-web-client": "^0.1.2",
    "jquery": "2.2.3",
    "lodash": "^4.17.10",
    "node-sass": "3.10.1",
    "open": "0.0.5",
    "prop-types": "^15.6.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-router": "3.2.1",
    "react-router-redux": "4.0.4",
    "react-tooltip": "3.6.1",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "sass-loader": "4.0.2",
    "spotify-web-api-js": "^0.21.2",
    "spotify-web-api-node": "^2.3.6",
    "toastr": "2.1.2",
    "webpack": "^1.15.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "3.3.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9"
  },
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json"
  ],
  "globals": {
    "__DEVELOPMENT__": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mbrashid62/Tidus"
  }
}
